  <table class="table">
	  <tr class="panel-heading row">
		  <td class="col-md-1"><%= Post.human_attribute_name("created_at") %></td>
		  <td class="col-md-6"><%= Post.human_attribute_name("title") %></td>
		  <td class="col-md-2"><%= Post.human_attribute_name("location") %></td>
		  <td class="col-md-1"><%= Post.human_attribute_name("link") %></td>
		  <td class="col-md-1"></td>
		  <td class="col-md-1"></td>
	  </tr>
	<% @posts.each do |f| %>
		<tr class="row">
			<td><%= f.created_at.strftime("%m/%d/%Y") %></td>
			<td><%= link_to f.title, post_path(f.id) %></td>
			<td><%= f.location %></td>
			<td>
				<%= link_to "http://#{f.link}", :target => '_blank', class: "btn-sm" do %>
    			    <i class="fa fa-external-link"></i>
				<% end %>
			</td>
			<td></i><%= link_to ' edit', edit_post_path(f.id), class: "fa fa-pencil-square-o" %></td>
			<td><%= link_to ' delete', post_path(f.id), class: "fa fa-trash-o", data: {confirm: "Are you sure you want to delete this post?"}, method: :delete %></td>
		</tr>
	<% end %>
  </table>
<div><%= will_paginate @posts, style: "margin: 0" %></div>